{
  "name": "my-accounting-app",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "restart": "npx kill-port 3000 && node server.js",
    "dev:server": "npx nodemon server.js",
    "dev": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/output.css --watch",
    "build": "webpack --mode production",
    "setup": "npm install && node scripts/setup.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:performance": "node test-performance.js",
    "test:api": "node test-api-performance.js",
    "test:all": "npm run test:performance && npm run test:api",
    "check:performance": "node quick-performance-check.js",
    "postinstall": "patch-package",
    "fix:console": "node replace-console-logs.js",
    "sync:product-html": "node scripts/sync-branchstock-from-product-html.js",
    "sync:product-html:check": "node scripts/sync-branchstock-from-product-html.js check",
    "sync:product-html:sync": "node scripts/sync-branchstock-from-product-html.js sync",
    "seed:roles": "node seeders/roleSeeder.js",
    "seed:roles:force": "node seeders/roleSeeder.js --force",
    "build:dev": "webpack --mode development",
    "build:analyze": "ANALYZE=true webpack --mode production",
    "clean": "rm -rf public/dist node_modules/.cache",
    "optimize": "npm run clean && npm run build",
    "cache:clear": "node scripts/clear-cache.js"
  },
